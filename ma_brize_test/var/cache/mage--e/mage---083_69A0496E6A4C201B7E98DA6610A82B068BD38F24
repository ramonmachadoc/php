<script type="text/javascript">

$jq(document).ready(function() {
	
	$jq(".tab_content_home_categorytab_slider").hide();
	$jq(".tab_content_home_categorytab_slider:first").show(); 

	$jq("ul.tabs-home_categorytab_slider li").click(function() {
		$jq("ul.tabs-home_categorytab_slider li").removeClass("active");
		$jq(this).addClass("active");
		$jq(".tab_content_home_categorytab_slider").hide();
		var activeTab = $jq(this).find("p").attr('class'); 
		$jq("#"+activeTab).fadeIn(); 
	});
});

</script> 
